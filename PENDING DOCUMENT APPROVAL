--DÖKÜMAN GRUPLARINDA ONAYDA BEKLEYENLERİ GÖRMEK--
SELECT A.DESCRIPTION, A.CONNECTION_DESCRIPTION, A.GROUP_ID, A.PERSON_ID FROM IFSAPP.APPROVAL_ROUTING A
JOIN IFSAPP.DOCUMENT_GROUP_MEMBERS B
ON A.GROUP_ID  =B.GROUP_ID 



--IFSINFO İLE DÖKÜMAN GRUPLARINDA ONAYDA BEKLEYENLERİ GÖRMEK SONUCUNDA ALDIĞIMIZ SONUÇ LOBİ VERİ KAYNAK TASARIMCISINDA GÖZÜKMEZSE KONTROL ETTİĞİMİZ KOD--
SELECT * FROM IFSINFO.AYD_PEND_DOC_APPROVAL_IAL apdai 





SELECT * FROM IFSAPP.APPROVAL_ROUTING A --BELGE REVİZYONU--

SELECT *FROM IFSAPP.PERSON_INFO_ALL --KİŞİ KARTI--

SELECT * FROM IFSAPP.DOC_ISSUE_REFERENCE --BELGE REVİZYONU STATÜ DURUMU--

SELECT * FROM IFSAPP.DOCUMENT_GROUP_MEMBERS dgm --BELGE GRUBU--
